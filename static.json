{
    "root": "dist/",
    "https_only": true,
    "routes": {
      "/**": "index.html"
    },
    "proxies": {
      "/api/": {
        "origin": "http://localhost:9095/api/"
      }
    },
    "headers": {
      "/": {
        "Cache-Control": "no-store, no-cache, must-revalidate"
      },
      "**.js": {
        "Cache-Control": "public, max-age=31536000",
        "Access-Control-Allow-Origin": "cdn-instance.example.com"
      },
      "**.css": {
        "Cache-Control": "public, max-age=31536000",
        "Access-Control-Allow-Origin": "cdn-instance.example.com"
      }
    }
  }